[include]
  path = ~/.gitconfig_local
[alias]
  co = checkout
  st = status
  br = branch
  w  = whatchanged
  pf = push --force-with-lease
  amend = commit --amend
  unstage = reset HEAD --
[apply]
  whitespace = nowarn
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[core]
  excludesfile = ~/.gitignore
[diff]
  ignoresubmodules = untracked
  compactionHeuristic = true
[merge]
  summary = true
[push]
  default = upstream
[pull]
  rebase = true
[fetch]
  prune = true
[remote "origin"]
  push = HEAD
[core]
  excludesfile = ~/.gitignore
[branch]
  autosetuprebase = always
[hub]
  protocol = https
[submodule]
  fetchJobs = 4
[gpg]
  program = gpg1
[pager]
  branch = false